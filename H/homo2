import org.bukkit.entity.Player;

public class homo2 implements CommandExecutor {

	@Override
	public boolean onCommand(CommandSender sender, Command cmd, String label,
			String[] args) {

		Player p = (Player)sender;
		if (label.equalsIgnoreCase("homo2")){
		p.sendMessage(p.getName() + ChatColor.LIGHT_PURPLE + "ï¼š114514////");
		}
		return false;
	}
}
